antigen use oh-my-zsh

# personal plugins
antigen bundles <<EOBUNDLES

    # personal plugins
    garodriguezlp/gitpod@main bundles/p10k
    garodriguezlp/gitpod@main bundles/fzf
    garodriguezlp/gitpod@main bundles/tmux
    garodriguezlp/gitpod@main bundles/sdk

    # from oh-my-zsh's library
    fzf
    git
    github
    tmux
    vscode
    jenv
    sdk
    autojump
    docker
    kubectl
    thefuck

    # from zsh-users
    zsh-users/zsh-syntax-highlighting
    zsh-users/zsh-autosuggestions
    zsh-users/zsh-completions

EOBUNDLES

antigen theme romkatv/powerlevel10k
antigen apply
