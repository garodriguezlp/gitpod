antigen use oh-my-zsh

# personal plugins
antigen bundles <<EOBUNDLES

    # personal plugins
    garodriguezlp/gitpod-dotfiles@main bundles/p10k
    garodriguezlp/gitpod-dotfiles@main bundles/fzf
    garodriguezlp/gitpod-dotfiles@main bundles/tmux
    garodriguezlp/gitpod-dotfiles@main bundles/sdk

    # from oh-my-zsh's library
    fzf
    git
    github
    tmux
    vscode
    jenv
    sdk
    autojump

    # from zsh-users
    zsh-users/zsh-syntax-highlighting
    zsh-users/zsh-autosuggestions
    zsh-users/zsh-completions

EOBUNDLES

antigen theme romkatv/powerlevel10k
antigen apply
